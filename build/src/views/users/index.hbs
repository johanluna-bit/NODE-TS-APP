{{#if users}}
{{#each users}}
<div class="col-md-4">
    <div class="card">
        <div class="card-header">
            <h3>{{typeUser}}</h3>
        </div>
        <div class="card-body">
            <p>Id: {{id}}</p>
            <p>Name: {{name}}</p>
            <p>Last Name: {{lastname}}</p>
            <p>E-mail: {{email}}</p>
            <p>Adress: {{adress}}</p>
            <p>Description: {{description}}</p>
        </div>
    </div>
</div>
{{/each}}
{{/if}}
